
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id           Int           @id @default(autoincrement())
  email        String        @unique
  password     String
  name         String?
  roleId       Int
  role         Role          @relation(fields: [roleId], references: [id])
  avatar    String?
  gooleId String ? @unique
  facebookId String ? @unique
}
model Role {
  id    Int     @id @default(autoincrement())
  name  String  @unique
  users User[]
}
